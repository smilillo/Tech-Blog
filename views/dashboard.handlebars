<h2>Your Dashboard</h2>
{{#if posts.length}}
<section>
    {{#each posts as |post|}}
        {{> post_info post}}
    {{/each}}
</section>
{{else}}
<div>No posts available</div>
{{/if}}

<button type="submit" class="button" href="/dashboard/new">+ New Post</button>

<script src="../public/js/new-post.js"></script>
